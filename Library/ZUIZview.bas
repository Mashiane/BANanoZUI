Sub SetCoverImage(url As String)
	Dim imgKey As String = $"${mName}img"$
	Dim img As ZUIZimage
	img.Initialize(mCallBack, imgKey, imgKey)
	img.AddAttr("slot", "image")
	img.Src = url
	img.Width = "100%"
	img.Height = "100%"
	sbText.Append(img.ToString)
End Sub
